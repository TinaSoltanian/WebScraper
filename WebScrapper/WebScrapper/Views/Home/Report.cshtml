@model IEnumerable<WebScrapper.Models.Request>
<div class="container">
    <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.NDCNo)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.MDunits)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.price)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ServiceDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.NursingHome)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Brand)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.RequestDate)
                </th>
            </tr>
            @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NDCNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MDunits)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.price)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ServiceDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NursingHome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Brand)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RequestDate)
            </td>
        </tr>

if (item.Response != null)
{
        <tr>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td>
                <table class="table-hover">
                    <tr>
                        <th>@Html.DisplayNameFor(res => res.Response.LabelName)</th>
                        <td>@Html.DisplayFor(modelItem => item.Response.LabelName) </td>
                    </tr>
                    <tr>
                        <th>@Html.DisplayNameFor(res => res.Response.PriceDate)</th>
                        <td> @Html.DisplayFor(modelItem => item.Response.PriceDate) </td>
                    </tr>
                    <tr>
                    <tr>
                        <th>@Html.DisplayNameFor(res => res.Response.MDunit)</th>
                        <td> @Html.DisplayFor(modelItem => item.Response.MDunit) </td>
                    </tr>
                    <tr>
                        <th>@Html.DisplayNameFor(res => res.Response.UnitPrice)</th>
                        <td> @Html.DisplayFor(modelItem => item.Response.UnitPrice) </td>
                    </tr>

                    <tr>
                        <th>@Html.DisplayNameFor(res => res.Response.ProductTotal)</th>
                        <td> @Html.DisplayFor(modelItem => item.Response.ProductTotal) </td>
                    </tr>

                </table>

            </td>
        </tr>
}
}
    </table>
</div>
